{
  "name": "savi.oooo",
  "version": "1.0.0",
  "description": "Personal website v2",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "browser-sync start --server \"dist\" --files \"dist\"",
    "build:js": "webpack --mode=production",
    "watch:js": "onchange \"src/js\" -- webpack --mode=development",
    "build:html": "posthtml -c posthtml.json",
    "watch:html": "onchange \"src/views\" -- npm run build:html",
    "css:scss": "sass --style compressed src/scss/main.scss dist/main.css",
    "css:autoprefixer": "postcss -u autoprefixer -r dist/*.css",
    "css:lint": "stylelint src/scss/*.scss  --custom-syntax postcss-scss",
    "build:css": "npm run css:lint && npm run css:scss && npm run css:autoprefixer",
    "watch:css": "onchange \"src/scss\" -- npm run build:css",
    "watch": "run-p serve watch:*",
    "build": "run-p build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SavioMenifer/savi.oooo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SavioMenifer/savi.oooo/issues"
  },
  "homepage": "https://github.com/SavioMenifer/savi.oooo#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@interactjs/types": "^1.10.11",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.9",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "eslint": "^7.32.0",
    "eslint-webpack-plugin": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss-cli": "^9.1.0",
    "postcss-scss": "^4.0.3",
    "posthtml": "^0.16.6",
    "posthtml-cli": "^0.10.0",
    "posthtml-modules": "^0.8.0",
    "sass": "^1.49.9",
    "style-loader": "^3.3.1",
    "stylelint": "^14.6.0",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@floating-ui/dom": "^0.4.1"
  }
}
